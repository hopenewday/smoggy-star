{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@redux/*": ["src/redux/*"],
      "@graphql/*": ["src/graphql/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "include": ["src", "astro.config.mjs"]
}